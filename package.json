{
    "name": "react-easy-preload",
    "version": "1.1.0",
    "description": "Custom react router switch for easy preloading data",
    "main": "dist/index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepublish": "cross-env NODE_ENV=production babel ./src --out-dir ./dist -s inline",
        "build": "babel ./src --out-dir ./dist -s inline && babel ./example/src --out-dir ./example/dist -s inline && npm run webpack",
        "webpack": "webpack ./example/dist/index.js -o ./example/dist --mode='development' --devtool source-map",
        "example": "http-server-spa ./example"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/victortrusov/react-easy-preload.git"
    },
    "files": [
        "dist/*"
    ],
    "keywords": [
        "react",
        "switch",
        "router",
        "react-router-dom",
        "preload",
        "prerender",
        "prefetch",
        "loading",
        "bar"
    ],
    "author": "Victor Trusov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/victortrusov/react-easy-preload/issues"
    },
    "homepage": "https://github.com/victortrusov/react-easy-preload#readme",
    "peerDependencies": {
        "react": "^16.8.0 || ^17.0.0",
        "react-dom": "^16.8.0 || ^17.0.0",
        "react-router-dom": "^5.0.0"
    },
    "dependencies": {
        "topbar": "^1.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.16",
        "@babel/core": "^7.13.16",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/preset-env": "^7.13.15",
        "@babel/preset-react": "^7.13.13",
        "cross-env": "7.0.3",
        "eslint": "^7.24.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-config-standard-react": "^11.0.1",
        "eslint-plugin-import": "^2.22.1",
        "http-server-spa": "1.3.0",
        "node": "15.14.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-router-dom": "5.2.0",
        "webpack": "5.35.0",
        "webpack-cli": "4.6.0"
    }
}
