{
    "name": "react-easy-preload",
    "version": "1.0.2",
    "description": "Custom react router switch for easy preloading data",
    "main": "dist/index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepublish": "cross-env NODE_ENV=production babel ./src --out-dir ./dist -s inline",
        "build": "babel ./src --out-dir ./dist -s inline && babel ./example/src --out-dir ./example/dist -s inline && npm run webpack",
        "webpack": "webpack ./example/dist/index.js -o ./example/dist/pack.js --mode='development' --devtool source-map",
        "example": "http-server-spa ./example"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/victortrusov/react-easy-preload.git"
    },
    "files": [
        "dist/*"
    ],
    "keywords": [
        "react",
        "switch",
        "router",
        "react-router-dom",
        "preload",
        "prerender",
        "prefetch",
        "loading",
        "bar"
    ],
    "author": "Victor Trusov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/victortrusov/react-easy-preload/issues"
    },
    "homepage": "https://github.com/victortrusov/react-easy-preload#readme",
    "peerDependencies": {
        "react": "^16.8.0",
        "react-dom": "^16.8.0",
        "react-router-dom": "^5.0.0"
    },
    "dependencies": {
        "topbar": "^0.1.4"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/preset-env": "^7.9.6",
        "@babel/preset-react": "^7.9.4",
        "cross-env": "7.0.2",
        "eslint": "^6.8.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-config-standard-react": "^9.2.0",
        "eslint-plugin-import": "^2.20.2",
        "http-server-spa": "1.3.0",
        "node": "14.0.0",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-router-dom": "5.1.2",
        "webpack": "4.43.0",
        "webpack-cli": "3.3.11"
    }
}
